{
  "env": {
    "browser": true,
    "es2021": true,
  },
  "extends": [
    "eslint:recommended", 
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    },
  },
  "plugins": ["react" ],
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error",
    "indent": ["error", 2],
    "quotes": ["error", "double"],
    "jest/valid-expect": 0
  }
}
